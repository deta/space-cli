    "version": "0.2.0",
    "configurations": [
        {
            "name": "space dev",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}",
            "args": [
                "dev",
                "--dir=${workspaceFolder}/example",
            ]
        },
        {
            "name": "space dev trigger",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}",
            "args": [
                "dev",
                "--dir=${workspaceFolder}/example",
                "trigger",
                "action"
            ]
        },
        {
            "name": "space dev up",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}",
            "args": [
                "dev",
                "up",
                "--dir=${workspaceFolder}/example",
                "example",
            ]
        },
        {
            "name": "space dev run",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}",
            "args": [
                "dev",
                "run",
                "--dir=${workspaceFolder}/example",
                "ls",
            ]
        },
        {
            "name": "space dev proxy",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}",
            "args": [
                "dev",
                "proxy",
                "--dir=${workspaceFolder}/example",
            ]
        }
    ]
}
